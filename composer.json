{
    "name": "setono/gls-webservice-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle that integrates the GLS webservice PHP SDK",
    "keywords": [
        "symfony",
        "setono",
        "gls"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "setono/gls-webservice-php-sdk": "^1.0.1",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/http-foundation": "^4.0"
    },
    "require-dev": {
        "localheinz/composer-normalize": "^1.1",
        "phpstan/phpstan": "^0.10.3",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\GlsWebserviceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\GlsWebserviceBundle\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse -c phpstan.neon -l max src",
        "check-style": "./vendor/bin/ecs check --ansi src/ tests/",
        "fix-style": "./vendor/bin/ecs check --fix --ansi src/ tests/",
        "test": "./vendor/bin/phpunit"
    }
}
