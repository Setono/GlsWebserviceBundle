{
    "name": "setono/gls-webservice-bundle",
    "description": "Symfony bundle that integrates the GLS webservice PHP SDK",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "setono",
        "gls"
    ],
    "require": {
        "php": ">=7.4",
        "ext-soap": "*",
        "setono/gls-webservice-php-sdk": "^1.3",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "nyholm/symfony-bundle-test": "^2.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.17",
        "psalm/plugin-symfony": "^3.1",
        "roave/security-advisories": "dev-latest",
        "setono/code-quality-pack": "^2.2"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\GlsWebserviceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\GlsWebserviceBundle\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
